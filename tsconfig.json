{   
    "include": ["*.ts"],
    "compilerOptions": {
        "allowJs": true,
        "allowUnusedLabels": true,
        "allowSyntheticDefaultImports": true,
        "exactOptionalPropertyTypes": true,
        "moduleResolution": "Node",
        "noFallthroughCasesInSwitch": true,
        "noImplicitReturns": true,
        "noImplicitAny": true,
        "noImplicitThis": true,
        "strictNullChecks": true,
        "target": "ESNext",
    }
}